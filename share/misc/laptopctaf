#SOUND
echo "### REMOVING SOUND MODULES"
 rmmod snd_pcm_oss
 rmmod snd_intel8x0 
 rmmod snd_mixer_oss
 rmmod snd_ac97_codec
 rmmod snd_pcm
 rmmod snd_timer
 rmmod snd_page_alloc
 rmmod snd
 rmmod soundcore

#NETWORK
echo "### REMOVING NETWORK MODULES"
 rmmod 8139too
 rmmod 8139cp
 rmmod mii
 rmmod pcspkr

#USB
echo "### REMOVING USB MODULES"
 rmmod ehci_hcd
 rmmod uhci_hcd
 rmmod usbcore

#CPUFREQ
echo "### FIXING CPUFREQ"
 /etc/init.d/cpudyn stop
 echo 600000 > /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq
 echo 600000 > /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq
