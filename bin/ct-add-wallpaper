#CTAFCONF

if [ $# -gt 0 ]
then
wallpp=$1
else
wallpp=1
fi

echo "add wallpaper $wallpp"
echo "to use it launch ct-profile, and set the wallpaper to what you want"
cp $wallpp ~/.ctafconf/perso/wallpaper/
