#purge .svn and .cvs
rpurge_svn ()
{
 (echo removing: ***/\.svn) 2>/dev/null;
 (rm -rf ***/\.svn) 2>/dev/null;
 echo ".svn directory recursively purged";
}

rpurge_svn
