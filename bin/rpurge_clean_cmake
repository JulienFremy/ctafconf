#recursive purge
rpurge_clean_cmake ()
{
 (echo removing: ***/CMakeFiles ***/cmake_install.cmake ***/Makefile ***/CMakeCache.txt) 2>/dev/null
 (rm -f ***/CMakeFiles ***/cmake_install.cmake ***/Makefile ***/CMakeCache.txt) 2>/dev/null;
 echo "directory recursively purged";
}

rpurge_clean_cmake
