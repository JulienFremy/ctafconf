#CTAFCONF


cd ~
tar --exclude ".svn" --exclude "user-profile" --exclude "user-profile.mine" --exclude "user-profile-previous" --exclude "user-profile.csh" -cvzf ctafconf-`date "+%Y%m%d"`.tgz .ctafconf

echo "tarball of the ctafconf: " ~/ctafconf-`date "+%Y%m%d"`.tgz
