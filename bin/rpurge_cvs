rpurge_cvs ()
{
 (echo removing: ***/\.cvs ***/CVS) 2>/dev/null
 (rm -rf ***/\.cvs ***/CVS) 2>/dev/null;
 echo ".cvs directory recursively purged";
}

rpurge_cvs
