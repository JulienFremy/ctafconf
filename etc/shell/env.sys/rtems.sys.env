# AUTOMAGIC START(DO NOT UNCOMMENT NOR REMOVE)


#edit variables to your need
var_set TMP_RTEMS_BSD "nds"
var_set TMP_RTEMS_PATH_BIN "/opt/rtems-4.8/bin"
var_set TMP_RTEMS_CONFIGURE_PARAM "--target=arm-rtems4.8 --disable-networking"
var_set TMP_RTEMS_MAKEFILE_PATH "/shared/src/rtems/rtems/btmp/arm-rtems4.8/c/nds/make"


# AUTOMAGIC END(DO NOT UNCOMMENT NOR REMOVE)
#############################################
##
##
##--## ENVIRONMENT ##-##--## ##--## ##--## ##
### do not edit this section
##
##add the rtems toolchain to PATH
env_prepend PATH "$TMP_RTEMS_PATH_BIN"
##
##variable needed to build a rtems "external" application out of the tree
env_set RTEMS_MAKEFILE_PATH "$TMP_RTEMS_MAKEFILE_PATH"
##
##
##
##--## ALIAS ##-## ##--## ##--## ##--## ##-##
### do not edit this section
##
##crtems to configure your build folder
alias_set crtems "../configure --prefix=`pwd`/../build $TMP_RTEMS_CONFIGURE_PARAM"
##
##mrtems to call make in your build folder
alias_set mrtems "make RTEMS_BSP=$TMP_RTEMS_BSD"
##
##
var_unset TMP_RTEMS_BSD
var_unset TMP_RTEMS_PATH_BIN
var_unset TMP_RTEMS_CONFIGURE_PARAM
var_unset TMP_RTEMS_MAKEFILE_PATH
