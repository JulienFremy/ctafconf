#nothing to configure for this file

#this file configure proxy at epita

#you dont have to edit it



##--## WARNING ##--## ##--## ##--## ##--## ##
## do not modify anything under this line, ##
## it will be overwriten by ct-installconf ##
## if you update                           ##
## USER CONFIGURATION END ##--## ##--## ## ##
# AUTOMAGIC END(DO NOT UNCOMMENT NOR REMOVE)



env_set NNTPSERVER 'news.epita.fr'

#specify the socks for runsocks
env_set SOCKS5_SERVER 'socks.epita.fr:1080'
env_set SOCKS5_USER $USER
env_set SOCKS5_PASSWD ~/.socks

#specify the socks for tsocks
env_set TSOCKS_USERNAME $USER
env_set TSOCKS_PASSORWD `cat ~/.socks`

#specify proxy (for apt, )
env_set http_proxy  'http://proxy.epita.fr:3128'
env_set ftp_proxy  'http://proxy.epita.fr:3128'
env_set no_proxy "epita.fr, epita.net, epitech.fr, epitech.net"

env_set MAIL "/u/all/$USER/mail/$USER"

test -d /u/prof/acu/pub/NetBSD/bin && env_set PATH /u/prof/acu/pub/NetBSD/bin:"$PATH"

