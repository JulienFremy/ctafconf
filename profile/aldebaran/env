#!/bin/sh
##

# Always use ccache if it's installed
null_which ccache && env_set CC  "ccache gcc"
null_which ccache && env_set CXX "ccache g++"

alias_set na "naogentoo -c naogentoo-atom"
alias_set ng "naogentoo -c naogentoo-geode"

alias_set tc "toc cmake"
alias_set tm "toc build"

alias_set sc "toc cmake -s"
alias_set sm "toc build -s"

alias_set tcr "toc cmake --release"
alias_set tmr "toc build --release"
